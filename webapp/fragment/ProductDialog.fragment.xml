<core:FragmentDefinition xmlns="sap.m"
xmlns:core="sap.ui.core">

<Dialog id="idProductDialog" title="{i18n>prodDialogTitle}" class="custDialog">
    <Text id="txtDialog" text="{i18n>dialogMsg}" class="sapUiSmallMargin" />
    <beginButton>   
        <Button id="idButtonCloseDialog" text="{i18n>dialogCloseButtonText}" press="onCloseDialog"/>
    </beginButton>
</Dialog>

</core:FragmentDefinition>